{
  "manifest_version": 2,

  "name": "HackSource Resource Creator",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "http://hack-source.herokuapp.com/",
    "http://127.0.0.1:3000/"
  ]
}